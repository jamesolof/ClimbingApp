<!DOCTYPE html>
<html ng-app="ClimbingApp" ng-strict-di>
<head>
    <meta charset="utf-8" />
    <base href="/" />
    <title>Hello World</title>
    <!-- external stylesheets-->
    <link rel="stylesheet" href="lib/bootstrap/dist/css/bootstrap.min.css">

    <!-- application stylesheets-->
    <link rel="stylesheet" href="css/styles.css">

</head>
<body ng-cloak ng-controller="ApplicationController as vm">

    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Seattle Rock Climbing</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li><a ui-sref="Home">Home</a></li>
                    <li><a ui-sref="About">About</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right" ng-if="!vm.isUserLoggedIn">
                    <li><a ui-sref="Register">Register</a></li>
                    <li><a ui-sref="Login">Log in</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right" ng-if="vm.isUserLoggedIn">
                    <li><a>{{vm.user.userName}}</a></li>
                    <li><a href="" ng-click="vm.logOut()">Sign Out</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>

    <ui-view></ui-view>



    <!-- external Dependencies-->
    <script src="lib/underscore/underscore-min.js"></script>
    <script src="lib/jquery/dist/jquery.min.js"></script>
    <script src="lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="lib/angular/angular.min.js"></script>
    <script src="lib/angular-resource/angular-resource.min.js"></script>
    <script src="lib/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script src="~/lib/angular-bootstrap/ui-bootstrap.min.js"></script>
    <script src="~/lib/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>

    <!--app Deps-->
    <script src="~/js/models/route.model.js"></script>
    <script src="~/js/models/location.model.js"></script>
    <script src="~/js/models/register.model.js"></script>
    <script src="~/js/models/login.view.model.js"></script>
    <script src="~/js/models/user.model.js"></script>

    <script src="~/js/services/registration.service.js"></script>
    <script src="~/js/services/locations.service.js"></script>
    <script src="~/js/services/routes.service.js"></script>
    <script src="~/js/services/user.service.js"></script>
    <script src="~/js/services/services.js"></script>

    <script src="~/js/views/locations/locations.controller.js"></script>
    <script src="~/js/views/locations/locations.config.js"></script>
    <script src="~/js/views/locations/locations.js"></script>

    <script src="~/js/views/register/register.controller.js"></script>
    <script src="~/js/views/register/register.config.js"></script>
    <script src="~/js/views/register/register.js"></script>

    <script src="~/js/views/login/login.controller.js"></script>
    <script src="~/js/views/login/login.config.js"></script>
    <script src="~/js/views/login/login.js"></script>

    <script src="~/js/views/about/about.controller.js"></script>
    <script src="~/js/views/about/about.config.js"></script>
    <script src="~/js/views/about/about.js"></script>

    <script src="~/js/views/home/home.controller.js"></script>
    <script src="~/js/views/home/home.config.js"></script>
    <script src="~/js/views/home/home.js"></script>
    <script src="~/js/views/views.js"></script>

    <script src="~/js/app.controller.js"></script>
    <script src="~/js/app.config.js"></script>
    <script src="~/js/app.js"></script>
</body>
</html>